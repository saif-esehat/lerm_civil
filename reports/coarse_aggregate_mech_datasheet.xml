<?xml version="1.0" encoding="utf-8"?>
<odoo>
<template id="coarse_aggregate_datasheet">
		<t t-call="web.html_container">
			<!-- <t t-call="web.basic_layout"> -->
			 <t t-call="lerm_civil.mechanical_data_sheet_header">
                <div style="text-align:center;font-size:18px">
                    Coarse Aggregate
                </div>
				<div class="container_eln" style="margin-top:5px">
					<div t-if="data.crushing_visible">


						<div style="text-align:center;font-size:18px">
							Crushing Value 
							<span>
								<t t-foreach="env['lerm.parameter.master'].search([('internal_id','=','ee2d3ead-3bf8-4ae5-8e5d-dfe983111f71')]).parameter_table" t-as="parameter">
									<t t-if="parameter.grade.id == eln.grade_id.id">
										<span t-field="parameter.specification"/>
									</t>
								</t>
							</span>
						</div>
						<table>
							<tr cols="12">
                                <td colspan="1">Sr.No
                                </td>
                                <td colspan="1">Sample
								</td>
                                <td colspan="1">Weight of Empty Cylinder in gms (W1)
								</td>
                                <td colspan="2">Total weight of oven dried ( 4.0 hrs ) aggregate sample filling the cylindrical measure in gms (W2)
								</td>
                                <td colspan="2">Total weight of aggeregate in the cylinder in gms (A = W2-W1)
								</td>
                                <td colspan="2">Weight of aggregate fines passing 2.36 mm sieve after  the application of Load gms (B)
								</td>
                                <td colspan="2">Weight of aggregate retained on 2.36 mm sieve after the test in gms (W3 = A-B)
								</td>
                                <td colspan="1">Aggregate Crushing value (B/A *100)
								</td>
                            </tr>
                            
							<t t-set="a" t-value="0"/>
							<tr t-foreach="data.crushing_value_child_lines" t-as="r1">
								<td colspan="1">
									<div>
										<span t-esc="a + 1" />
										<t t-set="a" t-value="a+1"/>
									</div>
                  				</td>
								<td colspan="1">
									<span t-esc="r1['sample_no']"/>
								</td>
								<td colspan="1">
									<span t-esc="r1['wt_of_cylinder']"/>
								</td>
								<td colspan="2">
									<span t-esc="r1['total_wt_of_dried']"/>
								</td>
                                <td colspan="2">
									<span t-esc="r1['total_wt_aggregate']"/>
								</td>
                                <td colspan="2">
									<span t-esc="r1['wt_of_aggregate_passing']"/>
								</td>
                                <td colspan="2">
									<span t-esc="r1['wt_of_aggregate_retained']"  t-options='{"widget": "float", "precision": 2}'/>
								</td>
                                <td colspan="z">
									<span t-esc="r1['crushing_value']"  t-options='{"widget": "float", "precision": 2}'/>
								</td>
						</tr>
						</table>
                        <table style="margin-top:5px">
                        <tr cols="12">
								<td colspan="6">Average Aggregate Crushing Value %:
									<![CDATA[&nbsp;]]>
								</td>
								<td colspan="6">
									<![CDATA[&nbsp;]]>
									<span t-field="data.average_crushing_value"  t-options='{"widget": "float", "precision": 2}'/>
								</td>
							</tr>
                        </table>

						
					</div>

