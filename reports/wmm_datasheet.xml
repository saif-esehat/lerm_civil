							<td colspan="2">
								<span t-esc="r['percent_retained']" t-options='{"widget": "float", "precision": 2}'/>
							</td>
							<td colspan="2">
								<span t-esc="r['cumulative_retained']"/>
							</td>
							<td colspan="2">
								<span t-esc="r['passing_percent']"/>
