<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mechanical_rcmt" model="ir.ui.view">
        <field name="name">mechanical.rcmt.form</field>
        <field name="model">mechanical.rcmt</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                   <group >
                        <h1>
                            <field name="name_rcmt" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                    <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id" />
                        <field name="eln_ref"/>
