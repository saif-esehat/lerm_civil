                            <field  name="initial_voltage1" context="{'default_initial_voltage1': 'Cell-01'}"  colspan="12"  nolabel="1"/>
                          </group>
                          <group colspan="18" class="oe_inline"  >
                            <label for="initial_voltage2" string="2" />
