<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mechanical_tile" model="ir.ui.view">
        <field name="name">mechanical.tile.form</field>
        <field name="model">mechanical.tile</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                   <group >
                        <h1>
                            <field name="name" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                    <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id" readonly="1"/>
                        <field name="eln_ref" readonly="1"/>
                         <field name="grade" readonly="1"/>
                         <field name="size" />
                         <field name="product_id" invisible="1"/>
                        <field name="parameter_id" invisible="1"/>
                    </group>
                    <group>
                      <field name="sample_parameters">
                        <tree>
                          <field name="parameter_name"/>
                          <field name="test_method"/>
                        </tree>
                      </field>
                    </group>
                   
                   <group style="white-space: nowrap; width:70%;">
                        <field name="tile_type" />
                    </group>

                    <group attrs="{'invisible': [('dimension_visible', '=', False)]}">
                        <h1>
                            <field name="dimension_name1" readonly="1" nolabel="1"/>
                            <field name="dimension_visible" invisible="1"/>
                        </h1>
                    </group>


                    <group attrs="{'invisible': [('dimension_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name='length' col="4"/>
                        <field name='width'  col="4"/> 
                        <field name='thickness' col="4"/>
                        <field name='diagonal' col="4"/>
                    </group>

                    <group attrs="{'invisible': [('dimension_visible', '=', False)]}">
                        <field name="child_lines" nolabel="1" >
                            <tree editable="bottom">
                                <field name='sr_no'/>
                                <field name='length1'/>
                                <field name='length2'/>
                                <field name='length3'/>
                                <field name='length4'/>

                                <field name='width1'/>
                                <field name='width2'/>
                                <field name='width3'/>
                                <field name='width4'/>

                                <field name='thickness1'/>
                                <field name='thickness2'/>
                                <field name='thickness3'/>
                                <field name='thickness4'/>
                                <field name='thickness5'/>
                                <field name='thickness6'/>
                                
                            </tree>
                           
                         </field> 
                           
                       
                    </group>

                    <group attrs="{'invisible': [('dimension_visible', '=', False)]}">
                        <field name='average_length' col="4"/>
                        <field name='average_width'  col="4"/> 
                        <field name='average_thickness' col="4"/>
                    </group>

                     <group attrs="{'invisible': [('dimension_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name='deviation_length' col="4"/>
                        <field name='deviation_width'  col="4"/> 
                        <field name='deviation_length_width' col="4"/>
                        <!-- <field name='deviation_thickness' col="4"/> -->
                    </group>


                    <group attrs="{'invisible': [('deviation_thickness_visible', '=', False)]}">
                        <h1>
                            <!-- <field name="dimension_name1" readonly="1" nolabel="1"/> -->
                            <field name="deviation_thickness_visible" invisible="1"/>
                        </h1>
                    </group>

                    <group attrs="{'invisible': [('deviation_thickness_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                       
                        <field name='deviation_thickness' col="4"/>
                    </group>

                     <group attrs="{'invisible': [('dimension_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name="requirement_length_width" />
                        <!-- <field name="requirement_thickness" /> -->
                        
                    </group>

                     <group attrs="{'invisible': [('deviation_thickness_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name="requirement_thickness" />
                        
                    </group>


                     <group attrs="{'invisible': [('dimension_visible', '=', False)]}">
                     <group attrs="{'invisible': [('dimension_visible', '=', False)]}" style="white-space: nowrap; width:70%;width:25%">
                                <field name='deviation_length_width_conformity'
                                       decoration-success="deviation_length_width_conformity == 'pass'" 
                                       decoration-danger="deviation_length_width_conformity == 'fail'" 
                                       readonly="1"/>
                        </group>
                          <group attrs="{'invisible': [('dimension_visible', '=', False)]}" style="white-space: nowrap; width:70%;width:25%">
                                <field name="deviation_length_width_nabl"
                                       decoration-success="deviation_length_width_nabl == 'pass'" 
                                       decoration-danger="deviation_length_width_nabl == 'fail'" 
                                       readonly="1"/>
                            </group>
                        </group>


                          <group attrs="{'invisible': [('deviation_thickness_visible', '=', False)]}">
                     <group attrs="{'invisible': [('deviation_thickness_visible', '=', False)]}" style="white-space: nowrap; width:70%;width:25%">
                                <field name='deviation_thickness_conformity'
                                       decoration-success="deviation_thickness_conformity == 'pass'" 
                                       decoration-danger="deviation_thickness_conformity == 'fail'" 
                                       readonly="1"/>
                        </group>
                          <group attrs="{'invisible': [('deviation_thickness_visible', '=', False)]}" style="white-space: nowrap; width:70%;width:25%">
                                <field name="deviation_thickness_nabl"
                                       decoration-success="deviation_thickness_nabl == 'pass'" 
                                       decoration-danger="deviation_thickness_nabl == 'fail'" 
                                       readonly="1"/>
                            </group>
                        </group>


<!-- Straightness -->

                      <group attrs="{'invisible': [('straightness_visible', '=', False)]}">
                        <h1>
                            <field name="straightness_name" readonly="1" nolabel="1"/>
                            <field name="straightness_visible" invisible="1"/>
                        </h1>
                    </group>

                    <group attrs="{'invisible': [('straightness_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name='sample_size' col="4"/>
                      
                    </group>


                       <group attrs="{'invisible': [('straightness_visible', '=', False)]}">
                        <field name="child_lines_straightness" nolabel="1" >
                            <tree editable="bottom">
                                <field name='sr_no'/>
                                <field name='straightness1'/>
                                <field name='straightness2'/>
                                <field name='straightness3'/>
                                <field name='straightness4'/>

                               
                            </tree>
                           
                         </field> 
                           
                       
                    </group>

                        <group attrs="{'invisible': [('straightness_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name='average_straightness' col="4"/>
                         <field name='maximum_straightness' col="4"/>
                         <field name='deviation_straightness' col="4"/>
                      
                    </group>

                    <group attrs="{'invisible': [('straightness_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name='requirement_straightness' col="4"/>
                      
                    </group>
                    
                    <group attrs="{'invisible': [('straightness_visible', '=', False)]}">
                    <group style="width:25%" attrs="{'invisible': [('straightness_visible', '=', False)]}">
                                <field name='deviation_straightness_conformity'
                                       decoration-success="deviation_straightness_conformity == 'pass'" 
                                       decoration-danger="deviation_straightness_conformity == 'fail'" 
                                       readonly="1"/>
                        </group>
                          <group style="width:25%" attrs="{'invisible': [('straightness_visible', '=', False)]}">
                                <field name="deviation_straightness_nabl"
                                       decoration-success="deviation_straightness_nabl == 'pass'" 
                                       decoration-danger="deviation_straightness_nabl == 'fail'" 
                                       readonly="1"/>
                        </group>
                        </group>



<!-- Rectangularity -->


                     <group attrs="{'invisible': [('rectangularity_visible', '=', False)]}">
