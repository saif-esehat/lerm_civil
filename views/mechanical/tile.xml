                         </field> 
                           
                       
                    </group>

                    <group attrs="{'invisible': [('warpage_visible', '=', False)]}" style="white-space: nowrap; width:70%;">
                        <field name='average_warpage' col="4"/>
                        <field name='maximum_warpage' col="4"/>
                         <field name='deviation_warpage' col="4"/>
                      
                    </group>
