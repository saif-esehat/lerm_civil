<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mechanical_moisture_content_concrete_man_hole_view" model="ir.ui.view">
        <field name="name">mechanical.moisture.content.concrete.man.hole.form</field>
        <field name="model">mechanical.moisture.content.concrete.man.hole</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id" readonly="1"/>
                        <field name="parameter_id"/>
                       
                    </group>
                    <group>
                        <h1>
                            <field name="name" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                    <field name="child_lines" nolabel="1" >
                            <tree editable="bottom">
                                <field name='sr_no'/>
                                <field name='wt_of_sample'/>
                                <field name='oven_dry_wt'/>
                                <field name='moisture_content'/>
                                <!-- <field name='average_tensile'/> -->
                            </tree>
                           
                    </field> 
                            <group>
                               <field name='average_block'/>
                            </group>
                     
                </sheet>
            </form>
        </field>
    </record>
   


    </data>
</odoo>