                            <field  name="measured_valuess1"  colspan="3"  nolabel="1"/>
                            <field  name="measured_valuess2"  colspan="3"  nolabel="1"/>
                            </group>
                            <group>
                            <field  name="measured_valuess3"  colspan="3"  nolabel="1"/>
                            <field  name="measured_valuess4"  colspan="3"  nolabel="1"/>  
                            </group>
                            <group>
                             <field name="average_measuredss" string="Average"/>
                             <field name="percent_flowss" string="% Flow"/>
                            </group>          
                            
                            </group>
                           
                        </div>
                   
                        
                    </group>
                    </group>
                      <!-- 28 Days Casting  -->
                    <group attrs="{'invisible': [('lime_reactivity_visible', '=', False)]}">
