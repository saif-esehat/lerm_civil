                            <field name="status_28daysss"/>
                      
                    </group>
                    
                    <group attrs="{'invisible': [('lime_reactivity_visible', '=', False)]}">

                      <field name="casting_dates_28dayss"/>
                      <field name="testing_dates_28dayss"/>

                      <field name="casting_28_dayss_tabless">
                        <tree editable="bottom">
                          <field name="lengthss"/>
                          <field name="widthss"/>
                          <field name="crosssectional_areass"/>
                          <field name="wt_of_cement_cubess"/>
                          <field name="crushing_loadss" />
                          <field name="compressive_strengthss" />
                        </tree>
                      </field>
                      <field name="average_casting_28daysss"/>
                      <field name="compressive_strength_28_days"/>
                    </group>
                    <!-- Fineness Blaine  -->
                    <group attrs="{'invisible': [('fineness_blaine_visible', '=', False)]}">
