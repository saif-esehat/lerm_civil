<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="mechanical_flyasch_normalconsistency" model="ir.ui.view">
        <field name="name">mechanical.flyasch.normalconsistency.form</field>
        <field name="model">mechanical.flyasch.normalconsistency</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                   <group >
                        <h1>
                            <field name="name_fly" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                    <group>
                        <field name="srf_id" invisible="1"/>
                        <field name="sample_id" />
                        <field name="eln_ref"/>
                        <field name="parameter_id" invisible="1"/>
                    </group>
                    <group>
                      <field name="sample_parameters">
                        <tree>
                          <field name="parameter_name"/>
                          <field name="test_method"/>
                        </tree>
                      </field>
                    </group>
                      <!-- Normal Consistency Start -->
                    
                    <group >
                            
                            <field name="tests" widget="many2many_tags" options="{'no_create':True,'no_open': True}"/>
                    </group>
                    <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <h1>
                            <field name="normal_consistency_name" readonly="1" nolabel="1"/>
                            <field name="normal_consistency_visible" invisible="1"/>
                        </h1>
                    </group>
                    <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                    <group>
                      <field name="temp_percent_normal"/>
                      <field name="humidity_percent_normal"/>
                    </group>
                    <group>
                      <field name="start_date_normal"/>
                      <field name="end_date_normal"/>
                    </group>
                    </group>
                     <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="gravity_of_flyash1" string="Specific Gravity of Flyash" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="gravity_of_flyash1" string="Trial 1" />
                            <field  name="gravity_of_flyash1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="gravity_of_flyash2" string="Trial 2" />
                            <field  name="gravity_of_flyash2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                     <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="gravity_of_cement1" string="Specific Gravity of Cement" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="gravity_of_cement1" string="" />
                            <field  name="gravity_of_cement1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="gravity_of_cement2" string="" />
                            <field  name="gravity_of_cement2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                     <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="fly_ash_n1" string="N" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="fly_ash_n1" string="" />
                            <field  name="fly_ash_n1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="fly_ash_n2" string="" />
                            <field  name="fly_ash_n2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                     <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="wt_of_flash_1" string="Wt. of  Flyash" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="wt_of_flash_1" string="" />
                            <field  name="wt_of_flash_1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="wt_of_flash_2" string="" />
                            <field  name="wt_of_flash_2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="wt_of_cement_1" string="Wt. of  Cement (g)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="wt_of_cement_1" string="" />
                            <field  name="wt_of_cement_1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="wt_of_cement_2" string="" />
                            <field  name="wt_of_cement_2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="total_wt_of_sample_fly_1" string="Total Weight of Sample(g)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="total_wt_of_sample_fly_1" string="" />
                            <field  name="total_wt_of_sample_fly_1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="total_wt_of_sample_fly_2" string="" />
                            <field  name="total_wt_of_sample_fly_2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                     <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="wt_of_water_required_fly_1" string="Wt.of water required (g)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="wt_of_water_required_fly_1" string="" />
                            <field  name="wt_of_water_required_fly_1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="wt_of_water_required_fly_2" string="" />
                            <field  name="wt_of_water_required_fly_2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                     <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="penetration_planger_fly_1" string="Penetraion of vicat's Plunger (mm)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="penetration_planger_fly_1" string="" />
                            <field  name="penetration_planger_fly_1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="penetration_planger_fly_2" string="" />
                            <field  name="penetration_planger_fly_2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    <group attrs="{'invisible': [('normal_consistency_visible', '=', False)]}">
                        <label for="normal_consistency_fly_1" string="Normal Consistency, %" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="normal_consistency_fly_1" string="" />
                            <field  name="normal_consistency_fly_1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="normal_consistency_fly_2" string="" />
                            <field  name="normal_consistency_fly_2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    
                    
                     <!-- Setting Time Start -->
                    <group>
                        <field name="setting_time_visible" readonly="1" invisible="1" />
                        <h1>
                            <field name="setting_time_name" readonly="1" nolabel="1"   attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        </h1>
                    </group>
                    <group  attrs="{'invisible': [('setting_time_visible', '=', False)]}">
                    <group>
                      <field name="temp_percent_setting"/>
                      <field name="humidity_percent_setting"/>
                    </group>
                    <group>
                      <field name="start_date_setting"/>
                      <field name="end_date_setting"/>
                    </group>
                    </group>
                     <group>
                        
                        <field name="total_wt_of_sample_setting_time" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        <field name="wt_of_water_required_setting_time" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                    </group>
                    <!-- Initial Setting Time -->
                    <group style="width:50%">
                        <h3>
                            <field name="initial_setting_time" readonly="1" nolabel="1"   attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        </h3>
                   
                        <field name="time_water_added" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        <field name="time_needle_fails" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        <field name="initial_setting_time_hours" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        <field name="initial_setting_time_minutes" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        <!-- <field name="rounded_initial_setting_time_in_minutes" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/> -->
                     </group>
                     <group style="width:50%">
                        <h3>
                            <field name="final_setting_time" readonly="1" nolabel="1"   attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        </h3>
                   
                        <field name="time_needle_make_impression" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        <field name="final_setting_time_hours" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>
                        <field name="final_setting_time_minutes" attrs="{'invisible': [('setting_time_visible', '=', False)]}"/>

                    </group>

                   <!-- Particles retained on 45 micron IS sieve (wet sieving)  -->

                    <group attrs="{'invisible': [('particles_retained_visible', '=', False)]}">
                        <h1>
                            <field name="particles_retained" readonly="1" nolabel="1"/>
                            <field name="particles_retained_visible" invisible="1"/>
                        </h1>
                    </group>
                    
                    <group attrs="{'invisible': [('particles_retained_visible', '=', False)]}">
                    <group>
                      <field name="temp_percent_retained"/>
                      <field name="humidity_percent_retained"/>
                    </group>
                    <group>
                      <field name="start_date_retained"/>
                      <field name="end_date_retained"/>
                    </group>
                    </group>
                    <group attrs="{'invisible': [('particles_retained_visible', '=', False)]}">
                      <field name="particles_retained_table">
                        <tree editable="bottom">
                          <field name="sr_no"/>
                          <field name="sample_wt"/>
                          <field name="retained_wt"/>
                          <field name="wt_retained"/>
                        </tree>
                      </field>
                      <field name="average_weight_retained"/>
                      <field name="prcent_retaind"/>
                    </group>
                     <!-- Soundness Test -->
                    <group attrs="{'invisible': [('soundness_visible', '=', False)]}">
                        <h1>
                            <field name="soundness_name_fly" readonly="1" nolabel="1"/>
                            <field name="soundness_visible" invisible="1"/>
                        </h1>
                    </group>
                    <group attrs="{'invisible': [('soundness_visible', '=', False)]}">
                    <group>
                      <field name="temp_percent_soundness"/>
                      <field name="humidity_percent_soundness"/>
                    </group>
                    <group>
                      <field name="start_date_soundness"/>
                      <field name="end_date_soundness"/>
                    </group>
                    </group>
                    <group attrs="{'invisible': [('soundness_visible', '=', False)]}">
                      <field name="wt_of_flyash_soundness"/>
                      <field name="wt_of_cement_soundness"/>
                      <field name="total_wt_sample_soundness"/>
                      <field name="wt_of_water_req_soundness"/>
                      <field name="soundness_table">
                        <tree editable="bottom">
                           <field name="sr_no"/>
                          <field name="initial_distance"/>
                          <field name="final_distance"/>
                          <field name="expansion"/>
                        </tree>
                      </field>
                      <field name="average_soundness"/>
                      <field name="expansion_soundness"/>
                      
                    </group>

                    <!-- Specific Gravity -->
                    <group attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                        <h1>
                            <field name="specigic_gravity_fly" readonly="1" nolabel="1"/>
                            <field name="specigic_gravity_visible" invisible="1"/>
                        </h1>
                    </group>
                    <group attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                    <group>
                      <field name="temp_percent_specific"/>
                      <field name="humidity_percent_specific"/>
                    </group>
                    <group>
                      <field name="start_date_specific"/>
                      <field name="end_date_specific"/>
                    </group>
                    </group>

                    <group attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                        <label for="wt_of_flyash_specific1" string="Weight of Flyash (g)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="wt_of_flyash_specific1" string="Trial 1" />
                            <field  name="wt_of_flyash_specific1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="wt_of_flyash_specific2" string="Trial 2" />
                            <field  name="wt_of_flyash_specific2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                     <group attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                        <label for="intial_volume_specific1" string="Initial Volume of kerosine (ml)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="intial_volume_specific1" string="" />
                            <field  name="intial_volume_specific1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="intial_volume_specific2" string="" />
                            <field  name="intial_volume_specific2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    <group attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                        <label for="final_volume_specific1" string="Final Volume of kerosine and Flyash (After immersion in constant water bath) (ml)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="final_volume_specific1" string="" />
                            <field  name="final_volume_specific1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="final_volume_specific2" string="" />
                            <field  name="final_volume_specific2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    <group attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                        <label for="displaced_volume1" string="Displaced volume (cm³)" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="displaced_volume1" string="" />
                            <field  name="displaced_volume1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="displaced_volume2" string="" />
                            <field  name="displaced_volume2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    <group attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                        <label for="specific_gravity1" string="Specific Gravity" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="specific_gravity1" string="" />
                            <field  name="specific_gravity1"  colspan="3"  nolabel="1"/>
                          </group>
                          <group colspan="1" class="oe_inline"  >
                            <label for="specific_gravity2" string="" />
                            <field  name="specific_gravity2"  colspan="3"  nolabel="1"/>
                          </group>
                        </div>
                    </group>
                    <group>
                    <group class="oe_inline oe_center" attrs="{'invisible': [('specigic_gravity_visible', '=', False)]}">
                        <label for="average_specific_gravity" string="Average" />
                        <field name="average_specific_gravity" nolabel="1" class="oe_center" />
                    </group>
                    </group>
                     <!-- compressive strength -->
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}">
                        <h1>
                            <field name="compressive_name" readonly="1" nolabel="1"/>
                            <field name="compressive_visible" invisible="1"/>
                        </h1>
                    </group>
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}">
                    <group>
                      <field name="temp_percent_compressive"/>
                      <field name="humidity_percent_compressive"/>
                    </group>
                    <group>
                      <field name="start_date_compressive"/>
                      <field name="end_date_compressive"/>
                    </group>
                    </group>
                  
                  
                   <group>
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}" style="width:50%" class="center-group">
                     <h4 class="center-heading">Compressive Strength of Test Mixture</h4>
                      <field  name="specific_garavity_flyash"  colspan="3" />
                      <field  name="specific_gravity_cement"  colspan="3" />
                      <field  name="n2"  colspan="3" />
                      <field  name="weight_of_flyash"  colspan="3" />
                      <field  name="wt_of_cement_comp"  colspan="3" />
                      <field  name="wt_of_standerd_comp1"  colspan="3" />
                      <field  name="wt_of_standerd_comp2"  colspan="3" />
                      <field  name="wt_of_standerd_comp3"  colspan="3" />
                      <field  name="quantity_water"  colspan="3" />
                    </group>
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}" style="width:50%" class="center-group">
                    <group>
                    <h4 class="center-heading">Calculation of Flow (Acceptable : 105 ± 5 %)</h4>
                    </group>
                       
                        <label for="measured_value1" string="Measured Values" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="measured_value1" string="" />
                            <group>
                            <field  name="measured_value1"  colspan="3"  nolabel="1"/>
                            <field  name="measured_value2"  colspan="3"  nolabel="1"/>
                            </group>
                            <group>
                            <field  name="measured_value3"  colspan="3"  nolabel="1"/>
                            <field  name="measured_value4"  colspan="3"  nolabel="1"/>  
                            </group>
                            <group>
                             <field name="average_measured" string="Average"/>
                             <field name="percent_flow" string="% Flow"/>
                            </group>          
                            
                            </group>
                           
                        </div>
                   
                        
                    </group>

                    </group>
                    
                     <!-- 28 Days Casting  -->
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}">
                        <h1>
                            <field name="casting_28_name" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                     <group attrs="{'invisible': [('compressive_visible', '=', False)]}">
                            <field name="status_28days"/>
                      
                    </group>
                    
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}">

                      <field name="casting_date_28days"/>
                      <field name="testing_date_28days"/>

                      <field name="casting_28_days_tables">
                        <tree editable="bottom">
                          <field name="length"/>
                          <field name="width"/>
                          <field name="crosssectional_area"/>
                          <field name="wt_of_cement_cube"/>
                          <field name="crushing_load" />
                          <field name="compressive_strength" />
                        </tree>
                      </field>
                      <field name="average_casting_28days"/>
                    </group>

                    <group>

                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}" style="width:50%" class="center-group">
                     <h4 class="center-heading">Compressive Strength of control Sample</h4>
                      <field  name="wt_of_cement_fly"  colspan="3" />
                      <field  name="wt_of_standared_grade1"  colspan="3" />
                      <field  name="wt_of_standared_grade2"  colspan="3" />
                      <field  name="wt_of_standared_grade3"  colspan="3" />
                      <field  name="total_wieght"  colspan="3" />
                      <field  name="quantity_water_flyash"  colspan="3" />
                    </group>
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}" style="width:50%" class="center-group">
                    <group>
                    <h4 class="center-heading">Calculation of Flow (Acceptable : 105 ± 5 %)</h4>
                    </group>
                       
                        <label for="measured_values1" string="Measured Values" />
                        <div class="address_format " >
                          <group colspan="1"  class="oe_inline">
                            <label for="measured_values1" string="" />
                            <group>
                            <field  name="measured_values1"  colspan="3"  nolabel="1"/>
                            <field  name="measured_values2"  colspan="3"  nolabel="1"/>
                            </group>
                            <group>
                            <field  name="measured_values3"  colspan="3"  nolabel="1"/>
                            <field  name="measured_values4"  colspan="3"  nolabel="1"/>  
                            </group>
                            <group>
                             <field name="average_measureds" string="Average"/>
                             <field name="percent_flows" string="% Flow"/>
                            </group>          
                            
                            </group>
                           
                        </div>
                   
                        
                    </group>
                    </group>
                      <!-- 28 Days Casting  -->
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}">
                        <h1>
                            <field name="casting_28_names" readonly="1" nolabel="1"/>
                        </h1>
                    </group>
                     <group attrs="{'invisible': [('compressive_visible', '=', False)]}">
                            <field name="status_28dayss"/>
                      
                    </group>
                    
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}">

                      <field name="casting_dates_28days"/>
                      <field name="testing_dates_28days"/>

                      <field name="casting_28_dayss_tables">
                        <tree editable="bottom">
                          <field name="lengths"/>
                          <field name="widths"/>
                          <field name="crosssectional_areas"/>
                          <field name="wt_of_cement_cubes"/>
                          <field name="crushing_loads" />
                          <field name="compressive_strengths" />
                        </tree>
                      </field>
                      <field name="average_casting_28dayss"/>
                    </group>
                    <group attrs="{'invisible': [('compressive_visible', '=', False)]}">
                    <field name="compressive_strength_of_sample"/>
                    </group>
                    <!-- Lime Reactivity -->
                    <group attrs="{'invisible': [('lime_reactivity_visible', '=', False)]}">
                        <h1>
                            <field name="lime_reactivity" readonly="1" nolabel="1"/>
                            <field name="lime_reactivity_visible" invisible="1"/>
                        </h1>
                    </group>
                    <group  attrs="{'invisible': [('lime_reactivity_visible', '=', False)]}">
                    <group>
                      <field name="temp_percent_lime"/>
                      <field name="humidity_percent_lime"/>
                    </group>
                    <group>
                      <field name="start_date_lime"/>
                      <field name="end_date_lime"/>
                    </group>
                    </group>
                    <group>
                     <group attrs="{'invisible': [('lime_reactivity_visible', '=', False)]}" style="width:50%" class="center-group">
                      <field  name="specific_gravity_fly_lime"  colspan="3" />
                      <field  name="specific_gravity_of_lime"  colspan="3" />
                      <field  name="m"  colspan="3" />
                      <field  name="wt_of_hydrent"  colspan="3" />
                      <field  name="wt_of_fly_lime"  colspan="3" />
                      <field  name="wt_of_standard_grade_lime1"  colspan="3" />
                      <field  name="wt_of_standard_grade_lime2"  colspan="3" />
                      <field  name="wt_of_standard_grade_lime3"  colspan="3" />
                      <field  name="quantity_water_lime"  colspan="3" />
                    </group>
                     <group attrs="{'invisible': [('lime_reactivity_visible', '=', False)]}" style="width:50%" class="center-group">
                    <group>
                    <h4 class="center-heading">Calculation of Flow (Acceptable : 70 ± 5 %)</h4>
                    </group>
                       
                        <label for="measured_valuess1" string="Measured Values" />
