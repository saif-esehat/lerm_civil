            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sample_no_daywise" model="ir.sequence">
            <field name="name">Sample Daywise Sequence</field>
            <field name="code">lerm.sample.daywise.seq</field>
            <field name="prefix">-</field>
            <field name="padding">0</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- cement opc  -->

        <record id="ulr_no_seqs" model="ir.sequence">
            <field name="name">ULR Sequence</field>
            <field name="code">sample.ulr.seq</field>
            <field name="prefix">(lab_certificate_no)%(y)s(lab_no_value)</field>
            <field name="suffix">F</field>
            <field name="padding">8</field>
            <field name="company_id" eval="False"/>
        </record>

